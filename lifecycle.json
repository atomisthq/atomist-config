{
  "configuration": {
    "push": {
      "renderers": [
        "push",
        "commit",
        "issue",
        "tag",
        "build",
        "application",
        "k8pod",
        "footer"
      ],
      "contributors": [
        "tag",
        "build",
        "pullrequest",
        "generic"
      ],
      "configuration": {
        "emoji-style": "atomist",
        "fingerprints": {
          "about-hint": "false",
          "render-unchanged": "true",
          "style": "fingerprint-inline",
          "data": "${file://fingerprints.json}"
        },
        "build": {
          "style": "decorator"
        }
      }
    }
  },
  "pullrequest": {
    "renderers": [
      "pullrequest",
      "commit",
      "status",
      "footer"
    ],
    "contributors": [
      "merge",
      "approve",
      "delete"
    ],
    "configuration": {
    }
  }
}
